@model Ferramenta_Scrumt.MODEL.Users

@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="wrapper">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-body">
                        <div class="profile-desk">
                            <h1>Profile</h1>
                            <span class="designation">Funcao</span>
                            <p>
                                @Session["nomeUsuarioLogado"].ToString()
                            </p>
                            <a class="btn btn-post pull-left" href="#"> <i class="fa fa-check"></i> Ver Membros Cadastrados</a>

                            <ul class="p-social-link pull-right">
                                <li>
                                    <a href="#">
                                        <i class="fa fa-facebook"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-twitter"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-google-plus"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>